  "description": "mockServer for web",
  "keywords": [
    "mock",
    "mock server",
    "express",
    "webpack",
    "socket",
    "socket.io"
  ],
  "homepage": "https://github.com/liangskyli/mock#readme",
  "bugs": {
    "url": "https://github.com/liangskyli/mock/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangskyli/mock.git"
  },
  "license": "MIT",
  "author": "liangsky <640634387@qq.com>",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "bin": {
    "mock-server": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin",
    "docs",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rollup --config=./rollup.config.js",
    "start-esm": "cross-env DEBUG=mock:* tsx test/esm/server.ts",
    "start": "cross-env DEBUG=mock:* tsx test/server.mts",
    "test-http-cli": "cross-env DEBUG=mock:* node bin/index.js -d ./test -e mock/b.mts -c test/mock.config.mts",
    "test-http-cli-esm-config": "cross-env DEBUG=mock:* node bin/index.js -d ./test -c ./test/esm/mock.config.ts",
    "test:middleware": "tsx test/middleware.ts",
    "test:webpack-middleware": "tsx test/webpack-middleware.ts",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "@liangskyli/utils": "workspace:*",
    "@types/express": "^5.0.0",
    "body-parser": "^2.2.0",
    "chokidar": "^4.0.1",
    "commander": "^13.1.0",
    "express": "^5.1.0",
    "glob": "^10.4.5",
    "multer": "^2.0.0",
    "path-to-regexp": "^8.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@liangskyli/mock": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/multer": "^2.0.0",
    "better-mock": "^0.3.7",
    "socket.io-client": "^4.8.1",
    "webpack": "^5.97.1",
    "webpack-dev-server": "^5.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
