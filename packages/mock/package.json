{
  "name": "@liangskyli/mock",
  "version": "0.9.0-beta.1",
  "description": "mockServer for web",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangskyli/mock.git"
  },
  "keywords": [
    "mock",
    "mock server",
    "express",
    "webpack",
    "socket",
    "socket.io"
  ],
  "author": "liangsky",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "bin",
    "docs"
  ],
  "bin": {
    "mock-server": "./bin/index.js"
  },
  "scripts": {
    "dev": "tsc --watch",
    "start": "cross-env DEBUG=mock:utils cross-env TS_NODE_PROJECT=test/tsconfig.json node -r ts-node/register test/server.ts",
    "test-http-cli": "cross-env DEBUG=mock:utils cross-env TS_NODE_PROJECT=test/tsconfig.json node -r ts-node/register test/http-cli.ts",
    "test:middleware": "cross-env TS_NODE_PROJECT=test/tsconfig.json node -r ts-node/register test/middleware.ts",
    "test:webpack-middleware": "cross-env TS_NODE_PROJECT=test/tsconfig.json node -r ts-node/register test/webpack-middleware.ts",
    "build": "npm run clean && npm run compile",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "prepublish": "npm run build",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/register": "^7.16.9",
    "@liangskyli/utils": "^0.9.0-beta.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "body-parser": "^1.19.1",
    "chokidar": "^3.5.3",
    "commander": "^8.3.0",
    "cross-spawn": "^7.0.3",
    "express": "^4.17.2",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "path-to-regexp": "^6.2.0",
    "socket.io": "^4.4.1",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.178",
    "@types/mockjs": "^1.0.5",
    "@types/multer": "^1.4.7",
    "cross-env": "^7.0.3",
    "gts": "^3.1.0",
    "mockjs": "^1.1.0",
    "socket.io-client": "^4.4.1",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-dev-server": "^4.7.2"
  }
}
