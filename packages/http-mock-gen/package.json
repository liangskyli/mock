{
  "name": "@liangskyli/http-mock-gen",
  "version": "3.2.0-beta.0",
  "description": "http mock gen for web mockServer",
  "keywords": [
    "mock",
    "http mock generate"
  ],
  "homepage": "https://github.com/liangskyli/mock#readme",
  "bugs": {
