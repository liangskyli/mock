{
  "name": "@liangskyli/grpc-mock",
  "version": "6.0.0",
  "description": "grpc mock for bff",
  "keywords": [
    "grpc",
    "grpc mock",
    "grpc mock server"
  ],
  "homepage": "https://github.com/liangskyli/mock#readme",
  "bugs": {
    "url": "https://github.com/liangskyli/mock/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangskyli/mock.git"
  },
  "license": "MIT",
  "author": "liangsky <640634387@qq.com>",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "bin": {
    "grpc-mock": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin",
    "docs",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rollup --config=./rollup.config.js",
    "test-grpc-express-app": "tsx test/grpc/app.mts",
    "test-grpc-mock-code-gen": "tsx test/grpc/gen.mts",
    "test-grpc-mock-code-gen-cli": "node bin/index.js code-gen -c test/grpc/mock.config.cli.mts",
    "test-grpc-mock-server": "tsx test/grpc/server.mts",
    "test-grpc-mock-server-cli": "node bin/index.js server-start -w true -c test/grpc/mock.config.cli.mts",
    "update:deps": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.8.0",
    "@liangskyli/utils": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "commander": "^13.1.0",
    "cross-spawn": "^7.0.6",
    "fs-extra": "^11.3.2",
    "nodemon": "^3.1.11",
    "protobufjs": "^7.5.4"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@liangskyli/grpc-mock": "workspace:*",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^5.0.5",
    "better-mock": "^0.3.7",
    "express": "^5.1.0",
    "grpc": "^1.24.11"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.12.4"
  },
  "peerDependenciesMeta": {
    "@grpc/grpc-js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
