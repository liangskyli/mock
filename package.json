{
  "name": "@liangsky/mock",
  "version": "0.1.1",
  "description": "mockServer for web",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangskyli/mock.git"
  },
  "keywords": [
    "mock"
  ],
  "author": "liangsky",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "bin"
  ],
  "bin": {
    "mock-server": "./bin/index.js"
  },
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=test/tsconfig.test.json node -r ts-node/register test/mock.ts",
    "test-http-cli": "cross-env TS_NODE_PROJECT=test/tsconfig.test.json node -r ts-node/register test/http-cli.ts",
    "build": "npm run clean && npm run compile",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "prepublish": "npm run build",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "commander": "^8.3.0",
    "cross-spawn": "^7.0.3",
    "umi": "^3.5.20",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@umijs/fabric": "^2.8.1",
    "gts": "^3.1.0",
    "lint-staged": "^11.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
