{
  "name": "@liangskyli/mock",
  "version": "0.3.0",
  "description": "mockServer for web",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liangskyli/mock.git"
  },
  "keywords": [
    "mock",
    "mock server",
    "express",
    "webpack",
    "socket",
    "socket.io"
  ],
  "author": "liangsky",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "bin",
    "docs"
  ],
  "bin": {
    "mock-server": "./bin/index.js"
  },
  "scripts": {
    "dev": "tsc --watch",
    "start": "cross-env DEBUG=umi:preset-build-in:mock:utils cross-env TS_NODE_PROJECT=test/tsconfig.test.json node -r ts-node/register test/server.ts",
    "test-http-cli": "cross-env DEBUG=umi:preset-build-in:mock:utils cross-env TS_NODE_PROJECT=test/tsconfig.test.json node -r ts-node/register test/http-cli.ts",
    "test:middleware": "cross-env TS_NODE_PROJECT=test/tsconfig.test.json node -r ts-node/register test/middleware.ts",
    "test:webpack-middleware": "cross-env TS_NODE_PROJECT=test/tsconfig.test.json node -r ts-node/register test/webpack-middleware.ts",
    "build": "npm run clean && npm run compile",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "prepublish": "npm run build",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "commander": "^8.3.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.0.0",
    "socket.io": "^4.3.2",
    "umi": "^3.5.20",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/mockjs": "^1.0.4",
    "@types/webpack-dev-server": "^4.3.1",
    "@umijs/fabric": "^2.8.1",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "gts": "^3.1.0",
    "lint-staged": "^11.2.6",
    "mockjs": "^1.1.0",
    "socket.io-client": "^4.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.62.1",
    "webpack-dev-server": "^4.4.0"
  }
}
